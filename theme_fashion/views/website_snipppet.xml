<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="fashion_about" inherit_id="website.snippets" name="About">
        <xpath expr="//div[@id='snippet_feature']/div[hasclass('o_panel_body')]"
               position="inside">
            <t t-snippet="theme_fashion.s_fashion_about"
               t-thumbnail="/theme_fashion/static/src/images/snippets/about.jpg"/>
            <t t-snippet="theme_fashion.s_fashion_banner"
               t-thumbnail="/theme_fashion/static/src/images/snippets/index-banner-block.jpg"/>
            <t t-snippet="theme_fashion.s_fashion_cart"
               t-thumbnail="/theme_fashion/static/src/images/snippets/cart-block.jpg"/>
            <t t-snippet="theme_fashion.s_fashion_contact"
               t-thumbnail="/theme_fashion/static/src/images/snippets/contact-block.jpg"/>
            <t t-snippet="theme_fashion.s_fashion_preview"
               t-thumbnail="/theme_fashion/static/src/images/snippets/product-preview-block.jpg"/>
            <t t-snippet="theme_fashion.s_fashion_product_blog"
               t-thumbnail="/theme_fashion/static/src/images/snippets/blogs-block.jpg"/>
            <t t-snippet="theme_fashion.s_fashion_product"
               t-thumbnail="/theme_fashion/static/src/images/snippets/product-fashion-block.jpg"/>
            <t t-snippet="theme_fashion.s_fashion_slider"
               t-thumbnail="/theme_fashion/static/src/images/snippets/fashion-banner-block.jpg"/>
             <t t-snippet="theme_fashion.s_fashion_sliders"
               t-thumbnail="/theme_fashion/static/src/images/snippets/slider-block.jpg"/>
        </xpath>
    </template>
</odoo>