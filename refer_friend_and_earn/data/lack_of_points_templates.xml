<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--Template for giving the message to user that, they are not entered valid
        points-->
    <template id="lack_of_points_template">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="container">
                    <div class="col-md-12">
                        <div class="alert alert-danger">
                            Sorry !!!!!
                            <br/>
                            Please enter valid points or Set the Discount on
                            points
                        </div>
                        <a href='/shop/cart'>Go Back</a>
                    </div>
                </div>
            </div>
        </t>
    </template>
</odoo>
