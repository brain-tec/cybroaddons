<templates id="template" xml:space="preserve">
    <!--  Navbar  -->
    <t t-inherit="web.NavBar.AppsMenu" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('o_navbar_apps_menu')]" position="replace">
            <t t-if="!isScopedApp">
                <button data-hotkey="h" title="Home Menu" t-on-click="_onClickMenusPanel" style="background-color: transparent; border: transparent;">
                 <img t-att-src="'/theme_diwy/static/src/img/icon/topbar/left-arrow.svg'" alt="Back"/>
              </button>
            </t>
        </xpath>
    </t>
</templates>