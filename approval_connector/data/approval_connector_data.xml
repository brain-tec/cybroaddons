<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
<!--        Create a new sale type approval in the approval type-->
        <record id="approval_category_sale" model="approval.category">
            <field name="name">Create Sale Request</field>
            <field name="approval_type">sale</field>
            <field name="sequence">95</field>
            <field name="has_product">required</field>
            <field name="has_quantity">required</field>
            <field name="automated_sequence" eval="True"/>
            <field name="sequence_code">SALE</field>
        </record>
    </data>
</odoo>
