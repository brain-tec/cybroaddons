<?xml version="1.0" encoding="utf-8" ?>
<templates xml:space="preserve">
    <!-- RestrictDebug template inherits "web.DebugMenu" template to add "t-ref"-->
    <t t-name="RestrictDebug" t-inherit="web.DebugMenu"
       t-inherit-mode="extension" owl="1">
        <xpath expr="//Dropdown" position="attributes">
            <attribute name="t-ref">DebugDropDown</attribute>
        </xpath>
    </t>
</templates>
