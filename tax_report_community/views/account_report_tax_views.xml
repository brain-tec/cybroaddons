<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    kit account tax report view from  -->
    <record id="kit_account_tax_report_view_from" model="ir.ui.view">
        <field name="name">kit.account.tax.report.view.form</field>
        <field name="model">kit.account.tax.report</field>
        <field name="inherit_id"
               ref="base_accounting_kit.accounting_tax_report_view"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//button[@name='check_report']" position="after">
                    <button name="print_xls" string="Print Excel"  class="oe_highlight" type="object"/>
                </xpath>
            </data>
        </field>
    </record>
</odoo>